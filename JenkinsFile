pipeline {
  agent any
  parameters {
    gitParameter branchFilter: '.*', defaultValue: 'master', name: 'BRANCH', type: 'PT_BRANCH'
  }
  stages {
    stage('Git clone') {
      steps {
        git branch: "${params.BRANCH}", url: 'https://github.com/varnaa/SampleJenkinsJob/.git'
      }
    }
    stage('Preparation') {
      steps{
         echo 'Successfully cloned repository...'
      }
    }
    stage('Build') {
      steps{
        sh '''
          java--version
          echo "changing into directory src"
          cd src
          pwd
          java Main.java
        '''
      }
    }
    stage('Results') {
      steps{
        echo "Ending job ..."
      }
    }
  }
}
